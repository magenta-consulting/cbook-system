{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/beancomponent/BeanDevToolBundle"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "sonata-project/media-bundle": "^3.5",
        "sonata-project/doctrine-orm-admin-bundle": "^3.6",
        "sonata-project/admin-bundle": "^3.35",
        "friendsofsymfony/ckeditor-bundle": "^1.0",
        "sonata-project/formatter-bundle": "^3.5",
        "gesdinet/jwt-refresh-token-bundle": "^0.4",
        "lexik/jwt-authentication-bundle": "^2.4",
        "symfony/lts": "^4@dev",
        "sonata-project/classification-bundle": "^3.7.1",
        "knplabs/doctrine-behaviors": "^1.5",
        "aws/aws-sdk-php": "^3.56",
        "symfony/webpack-encore-pack": "^1.0"
    },
    "require-dev": {
        "beancomponent/devtool-bundle": "^0.1.2",
        "symfony/debug-pack": "*",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*",
        "friendsofphp/php-cs-fixer": "^2.2",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/console": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/yaml": "^4.0",
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Bean\\Bundle\\BookBundle\\": "libraries\\bundle\\BeanBookBundle\\src/",
            "Bean\\Bundle\\CreativeWorkBundle\\": "libraries\\bundle\\BeanCreativeWorkBundle\\src/",
            "Bean\\Bundle\\MediaBundle\\": "libraries\\bundle\\BeanMediaBundle\\src/",
            "Bean\\Bundle\\OrganizationBundle\\": "libraries\\bundle\\BeanOrganizationBundle\\src/",
            "Bean\\Bundle\\PersonBundle\\": "libraries\\bundle\\BeanPersonBundle\\src/",
            "Bean\\Bundle\\SonataMediaBundle\\": "libraries\\bundle\\BeanSonataMediaBundle\\src/",
            "Bean\\Bundle\\ThingBundle\\": "libraries\\bundle\\BeanThingBundle\\src/",
            "Magenta\\Bundle\\CBookAdminBundle\\": "libraries\\bundle\\MagentaCBookAdminBundle\\src/",
            "Magenta\\Bundle\\CBookJWTBundle\\": "libraries\\bundle\\MagentaCBookJWTBundle\\src/",
            "Magenta\\Bundle\\CBookModelBundle\\": "libraries\\bundle\\MagentaCBookModelBundle\\src/",
            "Bean\\Component\\Book\\": [
                "libraries\\component\\Book\\src/",
                "libraries\\component\\BookChapter\\src/",
                "libraries\\component\\BookPage\\src/"
            ],
            "Bean\\Component\\CreativeWork\\": "libraries\\component\\CreativeWork\\src/",
            "Bean\\Component\\Organization\\": [
                "libraries\\component\\IndividualMember\\src/",
                "libraries\\component\\IndividualMemberGroup\\src/",
                "libraries\\component\\Organization\\src/"
            ],
            "Bean\\Component\\Media\\": "libraries\\component\\Media\\src/",
            "Bean\\Component\\Page\\": "libraries\\component\\Page\\src/",
            "Bean\\Component\\Person\\": "libraries\\component\\Person\\src/",
            "Bean\\Component\\Thing\\": "libraries\\component\\Thing\\src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Bean\\Bundle\\BookBundle\\spec\\": "libraries\\bundle\\BeanBookBundle\\tests/spec/",
            "Bean\\Bundle\\CreativeWorkBundle\\spec\\": "libraries\\bundle\\BeanCreativeWorkBundle\\tests/spec/",
            "Bean\\Bundle\\MediaBundle\\spec\\": "libraries\\bundle\\BeanMediaBundle\\tests/spec/",
            "Bean\\Bundle\\OrganizationBundle\\spec\\": "libraries\\bundle\\BeanOrganizationBundle\\tests/spec/",
            "Bean\\Bundle\\PersonBundle\\spec\\": "libraries\\bundle\\BeanPersonBundle\\tests/spec/",
            "Bean\\Bundle\\SonataMediaBundle\\spec\\": "libraries\\bundle\\BeanSonataMediaBundle\\tests/spec/",
            "Bean\\Bundle\\ThingBundle\\spec\\": "libraries\\bundle\\BeanThingBundle\\tests/spec/",
            "Magenta\\Bundle\\CBookAdminBundle\\spec\\": "libraries\\bundle\\MagentaCBookAdminBundle\\tests/spec/",
            "Magenta\\Bundle\\CBookAdminBundle\\Tests\\": "libraries\\bundle\\MagentaCBookAdminBundle\\tests/units/",
            "Magenta\\Bundle\\CBookJWTBundle\\spec\\": "libraries\\bundle\\MagentaCBookJWTBundle\\tests/spec/",
            "Magenta\\Bundle\\CBookJWTBundle\\Tests\\": "libraries\\bundle\\MagentaCBookJWTBundle\\tests/units/",
            "Magenta\\Bundle\\CBookModelBundle\\spec\\": "libraries\\bundle\\MagentaCBookModelBundle\\tests/spec/",
            "Magenta\\Bundle\\CBookModelBundle\\Tests\\": "libraries\\bundle\\MagentaCBookModelBundle\\tests/units/",
            "Bean\\Component\\Book\\spec\\": [
                "libraries\\component\\Book\\tests/spec/",
                "libraries\\component\\BookChapter\\tests/spec/",
                "libraries\\component\\BookPage\\tests/spec/"
            ],
            "Bean\\Component\\CBookAsset\\spec\\": "libraries\\component\\CBookAsset\\tests/spec/",
            "Bean\\Component\\CreativeWork\\spec\\": "libraries\\component\\CreativeWork\\tests/spec/",
            "Bean\\Component\\Organization\\spec\\": [
                "libraries\\component\\IndividualMember\\tests/spec/",
                "libraries\\component\\IndividualMemberGroup\\tests/spec/",
                "libraries\\component\\Organization\\tests/spec/"
            ],
            "Bean\\Component\\Media\\spec\\": "libraries\\component\\Media\\tests/spec/",
            "Bean\\Component\\Page\\spec\\": "libraries\\component\\Page\\tests/spec/",
            "Bean\\Component\\Person\\spec\\": "libraries\\component\\Person\\tests/spec/",
            "Bean\\Component\\Thing\\spec\\": "libraries\\component\\Thing\\tests/spec/"
        }
    },
    "replace": {
        "egeloen/ckeditor-bundle": "*",
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.1.*"
        }
    }
}