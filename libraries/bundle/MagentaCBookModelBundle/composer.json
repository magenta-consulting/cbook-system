{
  "name": "magenta-consulting/cbook-model-bundle",
  "type": "symfony-bundle",
  "description": "Provides a Bundle Integration for the CBook Model.",
  "keywords": [
    "Symfony CBookModel Bundle"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Bean",
      "email": "peterbean@beancomponent.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/security-bundle": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/framework-bundle": "^4.0",
    "symfony/monolog-bundle": "^3.1",
    "symfony/lts": "^4@dev",

    "beancomponent/book-bundle": "^0.1",
    "sonata-project/media-bundle": "^3.13",
    "sonata-project/classification-bundle": "^3.7.1",
    "knplabs/doctrine-behaviors": "^1.5",
    "aws/aws-sdk-php": "^3.56",
    "symfony/dotenv": "*",
    "sonata-project/notification-orm-pack": "^1.0",
    "phpoffice/phpspreadsheet": "^1.5",
    "minishlink/web-push": "^4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.2",
    "swiftmailer/swiftmailer": "^6.0",
    "symfony/console": "^4.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/yaml": "^4.0",
    "phpunit/phpunit": "^6.5"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "Magenta\\Bundle\\CBookModelBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Magenta\\Bundle\\CBookModelBundle\\spec\\": "tests/spec/",
      "Magenta\\Bundle\\CBookModelBundle\\Tests\\": "tests/units/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
